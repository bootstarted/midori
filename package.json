{
  "name": "midori",
  "version": "0.4.4",
  "author": "Izaak Schroeder <izaak.schroeder@gmail.com>",
  "license": "CC0-1.0",
  "repository": "metalabdesign/midori",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run copy && npm run build && npm run import",
    "clean": "./node_modules/.bin/rimraf lib ./*.js ./*.map ./*.flow match internal",
    "build": "./node_modules/.bin/babel --copy-files -s -d lib src",
    "copy": "./node_modules/.bin/ncp src lib && ./node_modules/.bin/renamer --regex --find '$' --replace '.flow' 'lib/**/*.js'",
    "import": "./node_modules/.bin/ncp lib .",
    "test": "npm run lint && npm run spec && npm run flow",
    "spec": "NODE_ENV=test ./node_modules/.bin/_mocha -r test/helpers/chai.js -r adana-dump --compilers js:babel-core/register -R spec --recursive test/spec",
    "lint": "./node_modules/.bin/eslint .",
    "flow": "./node_modules/.bin/flow check"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "compression": "^1.7.0",
    "cookies": "^0.7.1",
    "http-proxy": "^1.16.2",
    "negotiator": "^0.6.1",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1",
    "parseurl": "^1.3.1",
    "path-to-regexp": "^2.0.0",
    "qs": "^6.5.0",
    "send": "^0.15.4",
    "useragent": "^2.2.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "adana-cli": "^0.1.1",
    "adana-dump": "^0.1.0",
    "adana-format-istanbul": "^0.1.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-adana": "^0.5.10",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "bl": "^1.2.1",
    "chai": "^3.4.1",
    "chai-http": "^1.0.0",
    "eslint": "^4.5.0",
    "eslint-config-metalab": "^7.0.1",
    "flow-bin": "^0.56.0",
    "mocha": "^2.3.4",
    "ncp": "^2.0.0",
    "purdy": "^3.0.2",
    "renamer": "^0.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
}
